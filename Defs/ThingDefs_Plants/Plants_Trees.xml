	<ThingDef ParentName="BT_TreeBase">
		<defName>BT_TreeCecropia</defName>
		<label>cecropia tree</label>
		<description>One of the most common rainforest trees. Grows very fast, but yields little usable wood.</description>
		<graphicData>
			<texPath>BT/Tree/TreeCecropia</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<Beauty>2</Beauty>
		</statBases>
		<plant>
			<sowTags>
				<li>/Tropic</li>
				<li>Clump</li>
			</sowTags>
			<growDays>14</growDays>
			<harvestedThingDef>WoodLog_Cecropia</harvestedThingDef>
			<harvestWork>550</harvestWork>
			<harvestYield>20</harvestYield>
			<visualSizeRange>
				<min>3.75</min>
				<max>4.25</max>
			</visualSizeRange>
		</plant>
		<ingestible>
			<nutrition>1.20</nutrition>
		</ingestible>
	</ThingDef>	
	
	<ThingDef ParentName="BT_TreeBase">
		<defName>BT_TreeOak</defName>
		<label>oak tree</label>
