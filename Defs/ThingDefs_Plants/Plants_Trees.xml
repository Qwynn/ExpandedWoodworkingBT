<Defs>

	<ThingDef ParentName="BT_TreeBase">
		<defName>BT_TreeBirch</defName>
		<label>green birch tree</label>
		<description>A northern tree known for its thin, paper-like bark.</description>
		<graphicData>
			<texPath>BT/Tree/TreeBirch</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.15, 0.3, 0.1)</volume>
			</shadowData>
		</graphicData>
		<statBases>
			<Beauty>3</Beauty>
		</statBases>
		<ingestible>
			<nutrition>1</nutrition>
		</ingestible>
		<plant>
			<sowTags>
				<li>/Soil</li>
				<li>Clump</li>
			</sowTags>
			<visualSizeRange>
				<min>2.0</min>
				<max>3.5</max>
			</visualSizeRange>
			<growDays>20</growDays>
			<harvestedThingDef>WoodLog_Birch</harvestedThingDef>
			<harvestWork>700</harvestWork>
			<harvestYield>30</harvestYield>
		</plant>
	</ThingDef>
  
